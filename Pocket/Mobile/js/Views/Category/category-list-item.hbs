{{#.}}
{{#if UserName}}
<li class="list-group-title">{{UserName}}</li>
{{/if}}
{{#each Categories}}
 {{#if Editable}}
<li class="swipeout category-swipeout-{{CategoryID}}" id="{{CategoryID}}">
    <a href="Pages/Category.html?id={{CategoryID}}" class="item-link item-content swipeout-content" data-category-id="{{CategoryID}}">
        {{#if IsShared}}
        <div class="item-media"><i class="icon ion-android-folder color-form-icon"></i></div>
        {{else}}
        <div class="item-media"><i class="icon ion-ios-bookmarks-outline"></i></div>
        {{/if}}
        <div class="item-inner">
            <div class="item-title-row">
                <div class="item-title">{{Name}}</div>
                <div class="item-after">{{Budget}}</div>
            </div>
        </div>
    </a>
    <div class="swipeout-actions-right">
        <a href="#" class="category-delete-link bg-red" data-id="{{CategoryID}}">Delete</a>
        {{#js_compare "!Template7.global.IsLocal"}}
        <a href="Pages/ResourceFriendList.html?ResourceType=3&ResourceID={{CategoryID}}" class="category-share-link bg-green">Share</a>
        {{else}}
        <a href="#" class="bg-green" data-id="{{CategoryID}}">Share &nbsp; <span class="badge bg-gray">Pro</span></a>
        {{/js_compare}}
        
    </div>
</li>
{{else}}
<li id="{{CategoryID}}">
    <div class="item-link item-content" data-category-id="{{CategoryID}}">
        <div class="item-media"><i class="icon ion-ios-bookmarks-outline"></i></div>
        <div class="item-inner">
            <div class="item-title-row">
                <div class="item-title">{{Name}}</div>
                <div class="item-after">{{Budget}}</div>
            </div>
        </div>
    </div>

</li>
{{/if}}
	
{{/each}}
{{/.}}