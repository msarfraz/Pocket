{{#.}}

<li class="list-group-title">{{#to_date ExpenseDateText}} {{/to_date}} <span class="color-red disabled ">{{TotalAmount}}</span>  </li>
{{#each Expenses}}
	<li class="swipeout expense-swipeout-{{ExpenseID}}" id="{{ExpenseID}}">
        <a href="Pages/Expense.html?id={{ExpenseID}}" class="swipeout-content item-link item-content">
            <div class="item-media "><span class="badge bg-red"> <i class="icon ion-ios-cart-outline color-white"></i></span></div>
             <div class="item-inner">

                <div class="item-title-row">
                    <div class="item-title">{{CategoryText}} - {{SubcategoryText}}</div>
                    <div class="item-after"><span class="color-red">{{Amount}}</span></div>
                </div>
                <div class="item-subtitle">{{AccountText}}
                {{#if UserName}}
                <span class="badge bg-green">{{UserName}}</span>
                {{/if}}
                </div>
            </div>
        </a>
        <div class="swipeout-actions-right">
            <a href="#" class="expense-delete-link bg-red" data-id="{{ExpenseID}}">Delete</a>
        </div>
</li>
{{/each}}

{{/.}}