{{#.}}
	
<li>
    <label class="label-checkbox item-content">
        <!-- Checked by default -->
        <input type="checkbox" value="{{FriendID}}" class="resfriend-shared"  data-fr-id="{{FriendID}}" {{#if Shared}} checked="checked" {{/if}}>
        <div class="item-media">
            <i class="icon icon-form-checkbox"></i>
        </div>
        <div class="item-inner">
            <div class="item-title">{{FriendName}}</div>
        </div>
    </label>
</li>
{{/.}}