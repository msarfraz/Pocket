{{#.}}
<li class="accordion-item cat-accordion">
    <a href="#" class="item-content item-link">
        <div class="item-inner">
            <div class="item-title">{{Name}}: {{Budget}}/{{Amount}}</div>
        </div>
    </a>
    <div class="accordion-item-content">
        <div class="content-block accordion-list">
            <ul>
                {{#each Subcategories}}
                <li class="accordion-item subcat-accordion">
                    <a href="#" class="item-content item-link">
                        <div class="item-inner">
                            <div class="item-title">{{Name}}: {{Budget}}/{{Amount}}</div>
                        </div>
                    </a>
                    <div class="accordion-item-content">
                        <div class="content-block">
                            {{#each Expenses}}
                            <p>{ExpenseID}</p>
                            {{/each}}
                        </div>



                    </div>
                </li>
                {{/each}}
            </ul>
        </div>
    </div>
</li>

	

{{/.}}